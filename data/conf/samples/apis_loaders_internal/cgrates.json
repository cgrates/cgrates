  },
],

"templates": {
  "baseTmpl": [
      {"tag": "Tenant", "path": "Tenant", "type": "*variable", "value": "~*req.0", "mandatory": true},
      {"tag": "ID", "path": "ID", "type": "*variable", "value": "~*req.1", "mandatory": true} 
  ]
},

"admins": {
  "enabled": true,
}
}