          "fields": [{"tag": "BaseTmpl","type": "*template", "value": "baseTmpl"}],
      },
      {
          "type": "*thresholds",						
          "file_name": "Thresholds.csv",				
          "fields": [{"tag": "BaseTmpl","type": "*template", "value": "baseTmpl"}],
      },
      {
        "type": "*trends",						
        "file_name": "Trends.csv",				
        "fields": [{"tag": "BaseTmpl","type": "*template", "value": "baseTmpl"}],
    },
    {
