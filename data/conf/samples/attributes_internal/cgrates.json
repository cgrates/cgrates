{

"db": {
  "db_conns": {
		"*default": {
      "db_type": "*internal",
        "opts":{
		    "internalDBRewriteInterval": "0s",
		    "internalDBDumpInterval": "0s"
	  }
	  },
	}
},

"attributes": {
  "enabled": true,
  "prefix_indexed_fields": ["*req.Destination"],
  "exists_indexed_fields": ["*opts.*usage"], 
  "notexists_indexed_fields": ["*req.ToR"]
},

"admins": {
  "enabled": true
},

"tpes": {
  "enabled": true
}

}