{
// CGRateS Configuration file
//


"general": {
	"node_id": "Engine1",
	"log_level": 7
},


"listen": {
 	"rpc_json": ":6012",
	"rpc_gob": ":6013",
	"http": ":6080",
},

"data_db": {
	"db_type": "*internal",
},

"caches":{
	"remote_conns": ["gob_cache"],						// the conns that are queried when the items are not found in cache
},   

"rpc_conns": {
	"conn1": {
		"strategy": "*first",
		"conns": [{"address": "127.0.0.1:6012", "transport":"*json"}],
	},
	"gob_cache": {
		"strategy": "*first",
		"conns": [
			{"address": "127.0.0.1:6013", "transport":"*gob"},
		],
	},
},

"attributes": {
	"enabled": true
},

"chargers": {
	"enabled": true,
	"attributes_conns": ["*internal"],
},


"admins": {
	"enabled": true,
	"caches_conns":["conn1"],
},

}
