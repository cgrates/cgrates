{
    // CGRateS Configuration file
    // Used in apis/filter_update_it_test.go
    
    "general": {
        "node_id": "fltr_update_e1_mongo",
    },
    
    "logger": {
        "level": 7
    },

    "listen": {
        "rpc_json": ":22012",
        "rpc_gob": ":22013",
        "http": ":22080",
    },
    
    
    "db": {
        "db_conns": {
            "*default": {
                "db_type": "mongo",
                "db_host": "127.0.0.1",	
                "db_port": 27017,
                "db_name": "10",
                "db_user": "cgrates"
            },
            "StorDB": {
                "db_type": "mongo",
                "db_host": "127.0.0.1",	
                "db_port": 27017,
                "db_name": "cgrates",
                "db_user": "cgrates"
            }
        },
        "items": {
            "*cdrs": {"limit": -1, "ttl": "", "static_ttl": false, "remote":false, "replicate":false, "dbConn": "StorDB"}
        }
    },

    
    "rpc_conns": {
        "cache_conn": {
            "strategy": "*broadcast_sync",
            "conns": [{"address": "127.0.0.1:22012", "transport": "*json"}, {"address": "127.0.0.1:2012", "transport": "*json"}],
        },
    },
    
    "attributes": {
        "enabled": true
    },
    
    "admins": {
        "enabled": true,
        "caches_conns":["cache_conn"],
    },
    
}
    