{

"logger": {
    "level": 7
},

"listen": {
    "rpc_json": ":2012",
    "rpc_gob": ":2013",
    "http": ":2080"
},

"db": {
    "db_conns": {
		"*default": {
            "db_type": "*internal",
            "opts":{
		"internalDBRewriteInterval": "0s",
		"internalDBDumpInterval": "0s"
	}
	    },
	}
},

"ees": {
    "enabled": true,
    "cache": {
        "*fileCSV": {"limit": -1, "ttl": "500ms", "static_ttl": false}
    },
    "exporters": [
        {
            "id": "CSVExporter1",
            "type": "*fileCSV",
            "export_path": "/tmp/CSVFile1",
            "attempts": 1,
            "blocker": true,
            "field_separator": ","
        },
        {
            "id": "CSVExporter2",
            "type": "*fileCSV",
            "export_path": "/tmp/CSVFile2",
            "attempts": 1,
            "field_separator": ","
        }
    ]
},


"admins": {
    "enabled": true
}
    
}