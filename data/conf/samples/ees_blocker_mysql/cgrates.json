{

    "logger": {
        "level": 7
    },
    
    "listen": {
        "rpc_json": ":2012",
        "rpc_gob": ":2013",
        "http": ":2080"
    },
        
    "db": {
        "db_conns": {
            "*default": {
                "db_type": "redis",
                "db_host": "127.0.0.1",
                "db_port": 6379,
                "db_name": "10",
                "db_user": "cgrates"
            },
            "StorDB": {
                "db_type": "mysql",
                "db_host": "127.0.0.1",
                "db_port": 3306,
                "db_name": "cgrates",
                "db_user": "cgrates",
                "db_password": "CGRateS.org"
            }
        },
        "items": {
            "*cdrs": {"limit": -1, "ttl": "", "static_ttl": false, "remote":false, "replicate":false, "dbConn": "StorDB"}
        }
    },

    "ees": {
        "enabled": true,
        "cache": {
            "*fileCSV": {"limit": -1, "ttl": "500ms", "static_ttl": false}
        },
        "exporters": [
            {
                "id": "CSVExporter1",
                "type": "*fileCSV",
                "export_path": "/tmp/CSVFile1",
                "attempts": 1,
                "blocker": true,
                "field_separator": ","
            },
            {
                "id": "CSVExporter2",
                "type": "*fileCSV",
                "export_path": "/tmp/CSVFile2",
                "attempts": 1,
                "field_separator": ","
            }
        ]
    },
    
    
    "admins": {
        "enabled": true
    }
    
}