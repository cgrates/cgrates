	"default_timezone": "UTC",
},


"listen": {
	"rpc_json": ":2012",
	"rpc_gob": ":2013",
	"http": ":2080",
},


"data_db": {
	"db_type": "mongo",
	"db_name": "10",
	"db_port": 27017,
},


"stor_db": {
	"db_type": "mongo",
	"db_name": "cgrates",
	"db_port": 27017,
},


"rals": {
	"enabled": true,
},


"schedulers": {
	"enabled": true,
},


"chargers": {
	"enabled": true,
	"attributes_conns": ["*internal"],
},


"resources": {
	"enabled": true,
},


"attributes": {
	"enabled": true,
},


"cdrs": {
	"enabled": true,
	"chargers_conns": ["*localhost"],
	"rals_conns": ["*internal"],
	"session_cost_retries": 0,
},


"sessions": {
	"enabled": true,
	"chargers_conns": ["*localhost"],
	"rals_conns": ["*internal"],
	"cdrs_conns": ["*internal"],
	"resources_conns": ["*internal"],
	"attributes_conns": ["*internal"],
