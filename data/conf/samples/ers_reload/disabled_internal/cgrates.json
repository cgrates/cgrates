{
// CGRateS Configuration file
//
// Used for SessionSv1 integration tests


"logger": {
    "level": 7
},


"listen": {
	"rpc_json": ":2012",
	"rpc_gob": ":2013",
	"http": ":2080",
},


"db": {
	"db_conns": {
		"*default": {
			"db_type": "*internal",	
				"opts":{
			"internalDBRewriteInterval": "0s",
			"internalDBDumpInterval": "0s"
		}
		},
	},
},

"rates": {
	"enabled": true,
},


"chargers": {
	"enabled": true,
	"attributes_conns": ["*internal"],
},


"resources": {
	"enabled": true,
	"store_interval": "-1",
},


"attributes": {
	"enabled": true,
},


"cdrs": {
	"enabled": true,
	"chargers_conns": ["*localhost"],
	"rates_conns": ["*internal"],
},


"sessions": {
	"enabled": true,
	"chargers_conns": ["*localhost"],
	"rates_conns": ["*internal"],
	"cdrs_conns": ["*internal"],
	"resources_conns": ["*internal"],
	"attributes_conns": ["*internal"],
},


"admins": {
	"enabled": true,
},


}
