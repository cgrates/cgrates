	"out_datadb_type": "mongo",
	"out_datadb_port": "27017",
	"out_datadb_name": "10",

	"users_filters":["Account"],
},


"admins": {
	"enabled": true,
},


"rates": {
	"enabled": true
},


"actions": {
	"enabled": true,
	"accounts_conns": ["*localhost"]
},


"accounts": {
	"enabled": true
},


"filters": {
	"stats_conns": ["*internal"],
	"resources_conns": ["*internal"],
	"accounts_conns": ["*internal"],
