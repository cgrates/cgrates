{

  "general": {
    "node_id": "AU_SITE",
  },

  "logger": {
    "level": 7
},

  "listen": {
    "rpc_json": ":3012",
    "rpc_gob": ":3013",
    "http": ":3080",
  },


  "rpc_conns": {
    "conn1": {
      "strategy": "*first",
      "conns": [{"address": "127.0.0.1:3012", "transport":"*json"}],
    },
    "rmtConn": {
      "strategy": "*first",
      "conns": [{"address": "127.0.0.1:4013", "transport":"*gob"}],
    },
    "*internal": {
      "strategy": "*first",
      "conns": [{"address": "*internal"}],
    }
  },


  "db": {
    "db_conns": {
		  "*default": {
        "db_type": "*internal",
        "remote_conns": ["rmtConn"],
	    },
      "StorDB": {						
			  "db_type": "mysql",			 
			  "db_host": "127.0.0.1",		
			  "db_port": 3306, 			
			  "db_name": "cgrates", 	
			  "db_user": "cgrates", 		
			  "db_password": "CGRateS.org" 
		  },
	  },
    "items":{
      "*accounts":{"remote":true, "dbConn": "*default"},
      "*filters": {"remote":true, "dbConn": "*default"},
      "*route_profiles":{"remote":true, "dbConn": "*default"},
      "*attribute_profiles":{"remote":true, "dbConn": "*default"},
      "*charger_profiles": {"remote":true, "dbConn": "*default"},
      "*load_ids":{"remote":true, "dbConn": "*default"},
      "*cdrs": {"limit": -1, "ttl": "", "static_ttl": false, "remote":false, "replicate":false, "dbConn": "StorDB"}
    },
   	"opts":{
		  "internalDBRewriteInterval": "0s",
		  "internalDBDumpInterval": "0s"
	  }
  },

  "rates": {
    "enabled": true,
  },


  "chargers": {
    "enabled": true,
  },

  "sessions": {
    "enabled": true,
    "rates_conns": ["*internal"],
    "chargers_conns": ["*internal"],
  },

  "admins": {
    "enabled": true,
    "caches_conns":["conn1"],
  }
}
