{

  "general": {
    "node_id": "AU_SITE",
  },

  "logger": {
    "level": 7
},

  "listen": {
    "rpc_json": ":3012",
    "rpc_gob": ":3013",
    "http": ":3080",
  },


  "rpc_conns": {
    "conn1": {
      "strategy": "*first",
      "conns": [{"address": "127.0.0.1:3012", "transport":"*json"}],
    },
    "rmtConn": {
      "strategy": "*first",
      "conns": [{"address": "127.0.0.1:4013", "transport":"*gob"}],
    },
    "*internal": {
      "strategy": "*first",
      "conns": [{"address": "*internal"}],
    }
  },


  "data_db": {
    "db_type": "*internal",
    "remote_conns": ["rmtConn"],
    "items":{
      "*accounts":{"remote":true},
      "*filters": {"remote":true},
      "*route_profiles":{"remote":true},
      "*attribute_profiles":{"remote":true},
      "*charger_profiles": {"remote":true},
      "*load_ids":{"remote":true},
      "*indexes":{"remote":true}
    }
  },

  "stor_db": {
    "db_password": "CGRateS.org"
  },

  "rates": {
    "enabled": true,
  },


  "chargers": {
    "enabled": true,
  },

  "sessions": {
    "enabled": true,
    "rates_conns": ["*internal"],
    "chargers_conns": ["*internal"],
  },

  "admins": {
    "enabled": true,
    "caches_conns":["conn1"],
  }
}
