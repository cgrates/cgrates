{
  "general": {
    "node_id": "US_SITE"
  },

  "logger": {
    "level": 7
  },

  "listen": {
    "rpc_json": ":4012",
    "rpc_gob": ":4013",
    "http": ":4080"
  },

  "rpc_conns": {
    "conn1": {
      "strategy": "*first",
      "conns": [{"address": "127.0.0.1:4012", "transport":"*json"}]
    },
    "rplConn": {
      "strategy": "*first",
      "conns": [{"address": "127.0.0.1:3013", "transport":"*gob"}]
    },
    "*internal": {
      "strategy": "*first",
      "conns": [{"address": "*internal"}]
    }
  },

  "db": {
    "db_conns": {
      "*default": {
        "db_type": "*redis",
        "db_host": "127.0.0.1",
        "db_port": 6379,
        "db_name": "11",
        "db_user": "cgrates",
        "replication_conns": ["rplConn"]
      },
      "StorDB": {
        "db_type": "mysql",
        "db_host": "127.0.0.1",
        "db_port": 3306,
        "db_name": "cgrates",
        "db_user": "cgrates",
        "db_password": "CGRateS.org"
      }
    },
    "items": {
      "*accounts":{"replicate":true, "dbConn": "*default"},
      "*resource_profiles":{"replicate":false, "dbConn": "*default"},
      "*resources":{"replicate":false, "dbConn": "*default"},
      "*statqueue_profiles": {"replicate":false, "dbConn": "*default"},
      "*statqueues": {"replicate":false, "dbConn": "*default"},
      "*threshold_profiles": {"replicate":false, "dbConn": "*default"},
      "*thresholds": {"replicate":false, "dbConn": "*default"},
      "*filters": {"replicate":false, "dbConn": "*default"},
      "*route_profiles":{"replicate":false, "dbConn": "*default"},
      "*attribute_profiles":{"replicate":false, "dbConn": "*default"},
      "*charger_profiles": {"replicate":false, "dbConn": "*default"},
      "*load_ids":{"replicate":false, "dbConn": "*default"},
      "*cdrs": {"limit": -1, "ttl": "", "static_ttl": false, "remote":false, "replicate":false, "dbConn": "StorDB"}
    }
  },

  "rates": {
    "enabled": true,
    "max_increments":3000000
  },


  "cdrs": {
    "enabled": true,
    "chargers_conns":["conn1"],
    "rates_conns": ["*internal"]
  },


  "chargers": {
    "enabled": true
  },


  "sessions": {
    "enabled": true,
    "listen_bijson": ":4014",
    "rates_conns": ["*internal"],
    "cdrs_conns": ["*internal"],
    "chargers_conns": ["*internal"]
  },


  "admins": {
    "enabled": true,
    "caches_conns":["conn1"],
  },

  "accounts": {
    "enabled": true
  }

}
