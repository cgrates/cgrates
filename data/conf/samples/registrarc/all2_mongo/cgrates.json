{

"general": {
	"node_id": "ALL2",
},

"logger": {
	"level": 7
},

"listen": {
 	"rpc_json": ":7012",
	"rpc_gob": ":7013",
	"http": ":7080"
},

"data_db": {
	"db_type": "mongo",
	"db_name": "12",
