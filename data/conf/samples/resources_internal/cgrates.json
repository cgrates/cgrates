{

"logger": {
	"level": 7
},

"db": {
	"db_conns": {
		"*default": {
			"db_type": "*internal",
			"opts":{
				"internalDBDumpPath": "/tmp/internal_db/db",
				"internalDBDumpInterval": "-1"
			}
    	}
	}
},

"actions": {
    "enabled": true
},

"thresholds": {
    "enabled": true,
    "actions_conns": ["*internal"]
},

"resources": {
  "enabled": true,
  "thresholds_conns": ["*internal"]
},

"admins": {
  "enabled": true
}

}