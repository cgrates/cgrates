{
    "general": {
        "log_level": 7,
        "reply_timeout": "50s"
    },
    
    "listen": {
        "rpc_json": ":2012",
        "rpc_gob": ":2013",
        "http": ":2080"
    },
    
    "data_db": {								
        "db_type": "redis",						
        "db_port": 6379, 						
        "db_name": "10"					
    },
    
 

    "ees": {
        "enabled": true,
        "exporters": [
            {
                "id": "thProcessEv1",
                "type": "*rpc",
                "opts": {
                    "rpcCodec": "*json", 
                    "connIDs": ["rplConn"],
                    "serviceMethod": "ThresholdSv1.ProcessEvent", 
                    "keyPath": "" ,
                    "certPath": "", 
                    "caPath": "",  
                    "tls": false, 
                    "rpcConnTimeout" : "1s", 
                    "rpcReplyTimeout":"5s"
                }
            },
            {
                "id": "thProcessEv2",
                "type": "*rpc",
                "opts": {
                    "rpcCodec": "*json", 
                    "connIDs": ["rplConn"],
                    "serviceMethod": "ThresholdSv1.ProcessEvent", 
                    "keyPath": "" ,
                    "certPath": "", 
                    "caPath": "",  
                    "tls": false, 
                    "rpcConnTimeout" : "1s", 
                    "rpcReplyTimeout":"5s",
                    "rpcAPIOpts": {
                        "*thdProfileIDs": ["THD_3"]
                    }
                }
            }
        ]
    },
    

    "rpc_conns": {
        "rplConn": {
            "strategy": "*broadcast_sync",
            "conns": [{"address": "127.0.0.1:22012", "transport": "*json"}]
        }
    },

    "sessions": {
        "enabled": true,
        "replication_conns": ["rplConn"]
    },
    
    "admins": {
        "enabled": true
    }
}