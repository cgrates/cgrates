{
    // CGRateS Configuration file
    //
    // Used for sessions/sessions_volume_discount_it_test.go
        
    "general": {
        "log_level": 7,
        "reply_timeout": "10s",
    },
        
    "data_db": {
        "db_type": "mongo",
        "db_name": "10",
        "db_port": 27017,
    },
    
    
    "stor_db": {
        "db_type": "mongo",
        "db_name": "cgrates",
        "db_port": 27017,
    },
          
    "sessions": {
        "enabled": true,
        "routes_conns": ["*localhost"],
        "cdrs_conns": ["*localhost"],
    },
        
    "attributes": {
        "enabled": true,
        "opts":{								// 			
            "*processRuns": [
                {
                    "Value": 2,
                },
            ],										
        },
    },

    "cdrs": {
        "enabled": true,
        "chargers_conns": ["*localhost"],
        "accounts_conns": ["*localhost"],
        "rates_conns": ["*localhost"],
        "ees_conns":["*localhost"],
        "opts": {								
            "*chargerS": [
                {
                    "FilterIDs": ["*notstring:~*req.Account:sipp"],
                    "Value": true,
                },
            ],
            "*rateS": [
                {
                    "FilterIDs": ["*notstring:~*req.Account:sipp|1001"],
                    "Value": true,
                },
                {
                    "FilterIDs": ["*string:~*req.RunID:customers"],
                    "Value": true,
                },
            ],	
            "*accountS": [
                {
                    "FilterIDs": ["*string:~*req.Account:1001", "*string:~*req.RunID:suppliers"],
                    "Value": true,
                },
            ],		
            "*eeS": [
                {
                    "Value": true,
                },
            ],
        },   
    },

    "ees": {
        "enabled": true,
        "exporters": [
            {
                "id": "MyLogger",
                "type": "*log",
            }
        ]
    },
        
    "chargers": {
        "enabled": true,
        "attributes_conns": ["*localhost"],
    },
        
    "rates": {
        "enabled": true,
        "string_indexed_fields": ["*req.RunID"],
        "rate_string_indexed_fields": [],
        "rate_prefix_indexed_fields": ["*req.Destination"],
        "opts":{
            "*profileIgnoreFilters": [
                {
                    "Value": "true",
                },
            ],	
        },
    },
        
    "accounts": {
        "enabled": true,
        "rates_conns": ["*localhost"],
        "opts":{				
            "*usage": [
                {
                    "Value": "1m",
                },
            ],
        },
    },
        
    "loaders": [
        {
            "id": "*default",
            "enabled": true,
            "tenant": "cgrates.org",
            "tp_in_dir": "/usr/share/cgrates/tariffplans/sessions_discount_volume/routes1",
            "tp_out_dir": "",
            "lockfile_path": "",
        },
    ],
        
    "routes": {
        "enabled": true,
        "accounts_conns": ["*localhost"],
        "rates_conns": ["*localhost"],
        "*ignoreErrors": [						
            {
                "Value": true,
            },
        ],
    },
        
    "admins": {
        "enabled": true,
    },
    
}