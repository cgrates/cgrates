{
// CGRateS Configuration file
//
// Used for SessionSv1 integration tests


"general": {
    "log_level": 7,
},


"listen": {
	"rpc_json": ":2012",
	"rpc_gob": ":2013",
	"http": ":2080",
},


"data_db": {
	"db_type": "mongo",
	"db_name": "10",
	"db_port": 27017,
},


"stor_db": {
	"db_type": "mongo",
	"db_name": "cgrates",
	"db_port": 27017,
},


"rals": {
	"enabled": true,
},


"schedulers": {
	"enabled": true,
},


"cdrs": {
	"enabled": true,
	"chargers_conns":["*internal"],
	"rals_conns": ["*internal"],
},


"chargers": {
	"enabled": true,
	"attributes_conns": ["*internal"],
},


"resources": {
	"enabled": true,
},


"attributes": {
	"enabled": true,
},


"thresholds": {
	"enabled": true,
	"store_interval": "1s",
},


"stats": {
	"enabled": true,
	"store_interval": "1s",
	"thresholds_conns": ["*internal"],
},


"suppliers": {
	"enabled": true,
},


"sessions": {
	"enabled": true,
	"session_ttl": "50ms",
	"chargers_conns": ["*internal"],
	"rals_conns": ["*internal"],
	"cdrs_conns": ["*internal"],
	"resources_conns": ["*internal"],
	"thresholds_conns": ["*internal"],
	"stats_conns": ["*internal"],
	"suppliers_conns": ["*internal"],
	"attributes_conns": ["*internal"],
},


"apiers": {
	"enabled": true,
	"scheduler_conns": ["*internal"],
},


}
