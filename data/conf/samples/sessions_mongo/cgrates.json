{
// CGRateS Configuration file
//
// Used for SessionSv1 integration tests


"general": {
	"reply_timeout": "10s"
},

"logger": {
	"level": 7
},

"listen": {
	"rpc_json": ":2012",
	"rpc_gob": ":2013",
	"http": ":2080",
},


"db": {
	"db_conns": {
		"*default": {
			"db_type": "mongo",
			"db_host": "127.0.0.1",	
			"db_port": 27017,
			"db_name": "10",
			"db_user": "cgrates"
		},
		"StorDB": {
			"db_type": "mongo",
			"db_host": "127.0.0.1",	
			"db_port": 27017,
			"db_name": "cgrates",
			"db_user": "cgrates"
		}
	},
	"items": {
		"*cdrs": {"limit": -1, "ttl": "", "static_ttl": false, "remote":false, "replicate":false, "dbConn": "StorDB"}
	}
},

"rates": {
	"enabled": true,
},


"cdrs": {
	"enabled": true,
	"session_cost_retries": 1,
	"chargers_conns":["*internal"],
	"rates_conns": ["*internal"],
},


"chargers": {
	"enabled": true,
	"attributes_conns": ["*internal"],
},


"resources": {
	"enabled": true,
},


"attributes": {
	"enabled": true,
},


"thresholds": {
	"enabled": true,
	"store_interval": "1s",
},


"stats": {
	"enabled": true,
	"store_interval": "1s",
	"thresholds_conns": ["*internal"],
},


"routes": {
	"enabled": true,
},


"sessions": {
	"enabled": true,
	"session_ttl": "50ms",
	"chargers_conns": ["*internal"],
	"rates_conns": ["*internal"],
	"cdrs_conns": ["*internal"],
	"resources_conns": ["*internal"],
	"thresholds_conns": ["*internal"],
	"stats_conns": ["*internal"],
	"routes_conns": ["*internal"],
	"attributes_conns": ["*internal"],
	"alterable_fields": ["Extra1"],
},


"admins": {
	"enabled": true,
},


}
