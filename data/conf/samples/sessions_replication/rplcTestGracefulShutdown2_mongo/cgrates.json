{

"general": {
	"node_id":"MasterReplication"
},

"logger": {
	"level": 7
},

"listen": {
	"rpc_json": "127.0.0.1:2012",
	"rpc_gob": "127.0.0.1:2013",
	"http": "127.0.0.1:2080"
},

"rpc_conns": {
	"rplConn": {
		"strategy": "*broadcast_sync",
		"conns": [{"address": "127.0.0.1:22012", "transport": "*json"}]
	}
},

"db": {
	"db_conns": {
		"*default": {
			"db_type": "mongo",
			"db_host": "127.0.0.1",	
			"db_port": 27017,
			"db_name": "10",
			"db_user": "cgrates"
		},
		"StorDB": {
			"db_type": "mongo",
			"db_host": "127.0.0.1",	
			"db_port": 27017,
			"db_name": "cgrates",
			"db_user": "cgrates"
		}
	},
	"items": {
		"*cdrs": {"limit": -1, "ttl": "", "static_ttl": false, "remote":false, "replicate":false, "dbConn": "StorDB"}
	}
},

"chargers": {
	"enabled": true
},

"sessions": {
	"enabled": true,		
	"replication_conns": ["rplConn"],
	"chargers_conns": ["*internal"]
},

"admins": {
	"enabled": true
}

}
