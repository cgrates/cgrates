{
    // CGRateS Configuration file
    // will be used in apis/stats_it_test.go
  
    "logger": {
      "level": 7
    },

    "db": {
      "db_conns": {
        "*default": {
          "db_type": "*internal",
                "opts":{
        "internalDBRewriteInterval": "0s",
        "internalDBDumpInterval": "0s"
      }
          }
      }
    },
  
    "actions": {
        "enabled": true,
        "thresholds_conns": ["*internal"],
    },

    "stats": {
        "enabled": true,
        "store_interval": "-1",
        "thresholds_conns": ["*internal"],
    },

    "thresholds": {
        "enabled": true,
        "store_interval": "-1",
        "actions_conns": ["*internal"],
    },
  
    "admins": {
      "enabled": true,
    },
  }