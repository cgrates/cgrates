{
// CGRateS Configuration file
//


"general": {
	"reply_timeout": "50s",
},

"logger": {
	"level": 7
},

"rpc_conns": {
	"conn1": {
		"strategy": "*first",
		"conns": [{"address": "127.0.0.1:2013", "transport":"*gob"}],
	},
},

"listen": {
	"rpc_json": ":2012",
	"rpc_gob": ":2013",
	"http": ":2080",
},


"db": {
	"db_conns": {
		"*default": {
			"db_type": "*internal",	
			"opts":{
		"internalDBRewriteInterval": "0s",
		"internalDBDumpInterval": "0s"
	}
    	}
	}
},



"rates": {
	"enabled": true,
},


"cdrs": {
	"enabled": true,
	"chargers_conns":["conn1"],
},


"attributes": {
	"enabled": true,
},


"chargers": {
	"enabled": true,
	"attributes_conns": ["conn1"],
},


"resources": {
	"enabled": true,
	"store_interval": "-1",
	"thresholds_conns": ["conn1"]
},


"stats": {
	"enabled": true,
	"store_interval": "-1",
	"thresholds_conns": ["conn1"],
},

"thresholds": {
	"enabled": true,
	"store_interval": "-1",
},


"routes": {
	"enabled": true,
	"prefix_indexed_fields":["*req.Destination"],
	"stats_conns": ["conn1"],
	"resources_conns": ["conn1"],
},


"sessions": {
	"enabled": true,
	"routes_conns": ["conn1"],
	"resources_conns": ["conn1"],
	"attributes_conns": ["conn1"],
	"rates_conns": ["conn1"],
	"cdrs_conns": ["conn1"],
	"chargers_conns": ["conn1"],
},


"admins": {
	"enabled": true,
},


"filters": {								
	"accounts_conns": ["*internal"],
},

"accounts": {
    "enabled": true
},

}
