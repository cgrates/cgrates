{
// CGRateS Configuration file


"general": {
	"reply_timeout": "30s",
},

"logger": {
	"level": 7
},

"rpc_conns": {
	"conn1": {
		"strategy": "*first",
		"conns": [{"address": "127.0.0.1:2013", "transport":"*gob"}],
	},
},


"listen": {
	"rpc_json": ":2012",
	"rpc_gob": ":2013",
	"http": ":2080",
},


"db": {
	"db_conns": {
		"*default": {
			"db_type": "mongo",
			"db_host": "127.0.0.1",	
			"db_port": 27017,
			"db_name": "10",
			"db_user": "cgrates"
		},
		"StorDB": {
			"db_type": "mongo",
			"db_host": "127.0.0.1",	
			"db_port": 27017,
			"db_name": "cgrates",
			"db_user": "cgrates"
		}
	},
	"items": {
		"*cdrs": {"limit": -1, "ttl": "", "static_ttl": false, "remote":false, "replicate":false, "dbConn": "StorDB"}
	}
},

"rates": {
	"enabled": true,
},


"cdrs": {
	"enabled": true,
},


"chargers": {
	"enabled": true,
	"attributes_conns": ["*internal"],
},


"resources": {
	"enabled": true,
	"store_interval": "1s",
	"thresholds_conns": ["*internal"]
},


"stats": {
	"enabled": true,
	"store_interval": "1s",
	"thresholds_conns": ["*internal"],
},


"thresholds": {
	"enabled": true,
	"store_interval": "1s",
},


"routes": {
	"enabled": true,
	"stats_conns": ["conn1"],
	"resources_conns": ["conn1"],
},


"attributes": {							// Attribute service
	"enabled": true,				// starts Attribute service: <true|false>.
},


"sessions": {
	"enabled": true,
	"rates_conns": ["*internal"],
	"cdrs_conns": ["*internal"],
	"chargers_conns": ["*internal"],
},


"migrator": {
	"out_datadb_type": "mongo",
	"out_datadb_port": "27017",
	"out_datadb_name": "10",

	"users_filters":["Account"],
},


"admins": {
	"enabled": true,
},


"filters": {								
	"accounts_conns": ["*internal"],
},

"accounts": {
    "enabled": true
},

}
