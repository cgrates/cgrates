	"store_interval": "1s",
	"thresholds_conns": [
		{"address": "*internal"}
	],
},


"stats": {
	"enabled": true,
	"store_interval": "1s",
	"thresholds_conns": [
		{"address": "*internal"}
	],
},

"thresholds": {
	"enabled": true,
	"store_interval": "1s",
},


"suppliers": {
	"enabled": true,
	"stats_conns": [
		{"address": "*internal"},
	],
},


"sessions": {
	"enabled": true,
	"suppliers_conns": [
		{"address": "*internal"}
	],
	"resources_conns": [
		{"address": "*internal"}
	],
	"attributes_conns": [
		{"address": "*internal"}
	],
