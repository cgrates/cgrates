{

"general": {
	"node_id": "Engine1"
},

"logger": {
	"level": 7
},

"listen": {
	"rpc_json": ":2012",
	"rpc_gob": ":2013",
	"http": ":2080"
},

"rpc_conns": {
	"cacheConn": {
		"strategy": "*broadcast",
		"conns": [
			{"address": "*internal"},
			{"address": "127.0.0.1:20212", "transport":"*json"}
		]
	}
},

"db": {
	"db_conns": {
		"*default": {
			"db_type": "redis",
			"db_host": "127.0.0.1",
			"db_port": 6379,
			"db_name": "10",
			"db_user": "cgrates"
		},
		"StorDB": {
			"db_type": "mysql",
			"db_host": "127.0.0.1",
			"db_port": 3306,
			"db_name": "cgrates",
			"db_user": "cgrates",
			"db_password": "CGRateS.org"
		}
	},
	"items": {
		"*cdrs": {"limit": -1, "ttl": "", "static_ttl": false, "remote":false, "replicate":false, "dbConn": "StorDB"}
	}
},

"admins": {
	"enabled": true,
	"caches_conns":["cacheConn"]
}

}
